{
    "package_organization": "pcdshub",
    "package_repo": "archstats",
    "ioc_organization": "{{ cookiecutter.package_organization }}",
    "ioc_name": "ioc-{{ cookiecutter.package_repo }}",
    "description": "{{ cookiecutter.ioc_name }}",
    "ioc_start_command": "{{ cookiecutter.package_repo }} --list-pvs",
    "pv_prefix": "{{ cookiecutter.ioc_name.upper().replace('-', ':') }}",
